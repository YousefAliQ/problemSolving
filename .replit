//Naive solution
const hasPairWithSum = (array, sum) => {
  for(let i=0; i<array.length-1; i++){
    for(let j=i+1; j<array.length; j++){
      if (array[i] + array[j] === sum){
        return true;
      }
    }
  }
  return false;
}

console.log(hasPairWithSum([1, 5, 4, 9], 7));